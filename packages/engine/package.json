{
	"name": "@octopus/engine",
	"version": "0.0.1",
	"files": [
		"dist"
	],
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"engine",
		"lowcode",
		"react",
		"visual-editor",
		"vite"
	],
	"scripts": {
		"start": "plugin-builder",
		"build": "cross-env BUILD_TYPE=PKG plugin-builder --build",
		"build:w": "cross-env BUILD_TYPE=PKG plugin-builder --build --watch",
		"build:analyze": "cross-env BUILD_TYPE=PKG  plugin-builder --build --analyze",
		"build:app": "export NODE_OPTIONS=--max-old-space-size=32768 && cross-env BUILD_TYPE=APP plugin-builder --build",
		"build:app:analyze": "cross-env BUILD_TYPE=APP plugin-builder --build --analyze",
		"lint": "eslint --ext .tsx,.ts src/",
		"prettier": "prettier --write ./src"
	},
	"dependencies": {
		"@ant-design/icons": "^4.8.0",
		"@dnd-kit/core": "^6.0.7",
		"@dnd-kit/modifiers": "^6.0.1",
		"@dnd-kit/sortable": "^7.0.2",
		"@dnd-kit/utilities": "^3.2.1",
		"@monaco-editor/react": "^4.5.1",
		"@octopus/layout": "^0.0.28",
		"@octopus/model": "^0.0.28",
		"@octopus/render": "^0.0.28",
		"antd": "^5.8.5",
		"consola": "^2.15.3",
		"i18next": "^22.1.5",
		"lodash-es": "^4.17.21",
		"mitt": "^3.0.0",
		"react-color": "^2.19.3"
	},
	"devDependencies": {
		"@octopus/plugin-builder": "^0.0.28",
		"@types/consola": "^2.2.5",
		"@types/lodash-es": "^4.17.6",
		"@types/react-color": "^3.0.6",
		"cross-env": "^7.0.3",
		"rc-select": "^14.4.0",
		"re-resizable": "^6.9.9",
		"react-i18next": "^12.1.1",
		"rollup-plugin-visualizer": "^5.8.3"
	}
}
